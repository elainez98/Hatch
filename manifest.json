{
  "name": "Basic Chrome extension",
  "description": "Print inside console when clicked",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Print inside console when clicked",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
